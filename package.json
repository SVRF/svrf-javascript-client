  "description": "SVRF API client",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SVRF/svrf-javascript-client.git"
  },
  "main": "lib/svrf-client.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "lib",
